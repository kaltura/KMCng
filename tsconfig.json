      "@kaltura-ng/kaltura-primeng-ui/*": [
        "../node_modules/@kaltura-ng/kaltura-primeng-ui/*"
      ],
      "@kaltura-ng/kaltura-ui": [
        "../node_modules/@kaltura-ng/kaltura-ui"
      ],
      "@kaltura-ng/kaltura-ui/*": [
        "../node_modules/@kaltura-ng/kaltura-ui/*"
      ],
      "@kaltura-ng/mc-shared": [
        "../node_modules/@kaltura-ng/mc-shared"
      ],
      "@kaltura-ng/mc-shared/*": [
        "../node_modules/@kaltura-ng/mc-shared/*"
