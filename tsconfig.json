      ],
      "ng2-page-scroll": [
        "../node_modules/ng2-page-scroll"
      ],
      "ng2-page-scroll/*": [
        "../node_modules/ng2-page-scroll/*"
      ],
      "ng2-translate": [
        "../node_modules/ng2-translate"
      ],
      "ng2-translate/*": [
        "../node_modules/ng2-translate/*"
      ],
      "ng2-webstorage": [
        "../node_modules/ng2-webstorage"
      ],
      "ng2-webstorage/*": [
        "../node_modules/ng2-webstorage/*"
      ],
      "normalize.css": [
        "../node_modules/normalize.css"
      ],
      "normalize.css/*": [
        "../node_modules/normalize.css/*"
      ],
      "primeng": [
        "../node_modules/primeng"
      ],
      "primeng/*": [
        "../node_modules/primeng/*"
      ],
      "ramda": [
        "../node_modules/ramda"
      ],
      "ramda/*": [
        "../node_modules/ramda/*"
      ],
      "rxjs": [
        "../node_modules/rxjs"
      ],
      "rxjs/*": [
        "../node_modules/rxjs/*"
      ],
      "seamless-immutable": [
        "../node_modules/seamless-immutable"
      ],
      "seamless-immutable/*": [
        "../node_modules/seamless-immutable/*"
      ],
      "ts-md5": [
        "../node_modules/ts-md5"
      ],
      "ts-md5/*": [
        "../node_modules/ts-md5/*"
      ],
      "tslib": [
        "../node_modules/tslib"
      ],
      "tslib/*": [
        "../node_modules/tslib/*"
      ],
      "web-animations-js": [
        "../node_modules/web-animations-js"
