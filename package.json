    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.1",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.4.4",
    "typescript": "2.0.3",
    "url-loader": "^0.5.7",
